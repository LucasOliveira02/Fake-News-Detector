{
    "version": 2,
    "builds": [
        {
            "src": "apps/api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "apps/web-client/dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "apps/api/main.py"
        }
    ]
}