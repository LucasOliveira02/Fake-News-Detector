{
    "version": 2,
    "outputDirectory": "dist",
    "functions": {
        "api/index.py": {
            "runtime": "@vercel/python",
            "excludeFiles": "{web-client,node_modules,venv,.venv,**/__pycache__}/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ]
}