{
    "version": 2,
    "functions": {
        "apps/api/main.py": {
            "runtime": "@vercel/python",
            "excludeFiles": "{apps/web-client,node_modules,venv,.venv,**/__pycache__}/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "apps/api/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "apps/web-client/$1"
        }
    ]
}