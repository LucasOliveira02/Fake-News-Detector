{
    "version": 2,
    "functions": {
        "apps/api/main.py": {
            "runtime": "@vercel/python"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "apps/api/main.py"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/web-client/$1"
        }
    ]
}